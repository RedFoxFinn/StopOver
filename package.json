{
  "name": "stopover",
  "appname": "StopOver",
  "appid": "rff.app.stopover",
  "homepage": "https://redfoxfinn.github.io/StopOver/",
  "version": "0.6.8",
  "private": false,
  "dependencies": {
    "@apollo/client": "^3.4.13",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.1",
    "@mui/lab": "^5.0.0-alpha.50",
    "@mui/material": "^5.0.1",
    "@reduxjs/toolkit": "^1.6.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "apollo-link-http": "^1.5.17",
    "axios": "^0.21.4",
    "cypress": "^8.4.1",
    "dotenv": "^10.0.0",
    "graphql": "^15.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.5",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.0.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "scripts": {
    "cm": "git-cz",
    "release": "standard-version",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test": "react-scripts test --colors --no-cache --coverage --verbose --notify --watchAll --noStackTrace --transformIgnorePatterns 'node_modules/*' ",
    "ci": "react-scripts test --colors --no-cache --coverage --verbose --noStackTrace --transformIgnorePatterns 'node_modules/*'",
    "eject": "react-scripts eject",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "cypress:open": "cypress open"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "airbnb",
      "airbnb/hooks"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!node_modules/",
      "!src/index.js",
      "!src/reportWebVitals.js",
      "!src/controllers/graphql/**/*.js",
      "!src/icons/**/*"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50
      }
    },
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "devDependencies": {
    "@commitlint/cli": "^13.2.0",
    "@commitlint/config-conventional": "^13.2.0",
    "babel-eslint": "^10.1.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "gh-pages": "^3.2.3",
    "history": "^5.0.1",
    "husky": "^7.0.2",
    "standard-version": "^9.3.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
